global
    daemon
    maxconn 512

defaults
    mode tcp
    timeout connect 5s
    timeout client 50s
    timeout server 50s

frontend http-in
    bind *:6443
    balance roundrobin
    default_backend masters

backend masters
    server master1 192.168.18.51:6443 maxconn 256
    server master2 192.168.18.52:6443 maxconn 256
    server master3 192.168.18.53:6443 maxconn 256
